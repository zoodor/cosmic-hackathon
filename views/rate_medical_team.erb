    <style>
	  @font-face {
		font-family: 'DK Cool Crayon';
		src: url('/fonts/dkcoolcrayon.ttf');
	  }
      .medical-professional {
      float:left;
      width:100px; 
	  height:100px;
      margin:10px;
      padding:10px;
	  border:0px;
          position:relative;
      }
      .medical-professional-image {
      position:absolute;
      left:0;
      top:0;
      z-index:-1;
      }
      .button {
      display: block;
      float:left;
	  font-family: 'DK Cool Crayon', 'Arial';
      }
    </style>
    <script src='/javascript/rate_medical_team.js'></script>

	<div class="medical-team-panel">
	    <% @professionals.each do |professional| %>
	    <div id="<%= professional.id %>" class="medical-professional" ondrop="drop(event)" ondragover="allowDrop(event)">
	      <img src="/images/<%= professional.image_url%>" class="medical-professional-image" width="100" height="100">
	    </div>
	    <% end %>
	</div>

	<p>
    	<img id="drag1" src="/images/star.jpg" draggable="true" ondragstart="drag(event)" width="35" height="35">
	</p>
	
    <div class="button" onclick="submit(<%= @patient_id %>)">Submit</div>
